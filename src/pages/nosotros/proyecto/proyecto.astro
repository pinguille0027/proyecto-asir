---
import Layeout from "../../../layouts/Layout.astro";
import { getCollection } from "astro:content";
const allPosts = await getCollection("proyecto");
---

<Layeout title="Blog del Proyecto">
  <h1>blog del proyecto</h1>
  <ul>
    {allPosts.map((post) => <li><a href={`/nosotros/proyecto/${post.slug}`}>{post.data.title}</a></li>)}
  </ul>
</Layeout>
